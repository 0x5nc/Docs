<section id="tabpanel_9ThmIvwZED-1_aspnetcore1x" role="tabpanel" data-tab="aspnetcore1x" aria-hidden="true" hidden="hidden">
<pre sourcefile="performance/response-compression.md" sourcestartlinenumber="116" sourceendlinenumber="116"><code class="lang-csharp" name="Main" highlight-lines="5,10-13">public void ConfigureServices(IServiceCollection services)
{
    services.AddResponseCompression(options =&gt;
    {
        options.Providers.Add&lt;GzipCompressionProvider&gt;();
        options.Providers.Add&lt;CustomCompressionProvider&gt;();
        options.MimeTypes = ResponseCompressionDefaults.MimeTypes.Concat(new[] { &quot;image/svg+xml&quot; });
    });

    services.Configure&lt;GzipCompressionProviderOptions&gt;(options =&gt; 
