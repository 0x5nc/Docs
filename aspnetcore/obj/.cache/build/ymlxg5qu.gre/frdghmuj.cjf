<section id="tabpanel_9ThmIvwZED-1_aspnetcore2x" role="tabpanel" data-tab="aspnetcore2x">
<pre sourcefile="performance/response-compression.md" sourcestartlinenumber="112" sourceendlinenumber="112"><code class="lang-csharp" name="Main" highlight-lines="3,8-11">services.AddResponseCompression(options =&gt;
{
    options.Providers.Add&lt;GzipCompressionProvider&gt;();
    options.Providers.Add&lt;CustomCompressionProvider&gt;();
    options.MimeTypes = ResponseCompressionDefaults.MimeTypes.Concat(new[] { &quot;image/svg+xml&quot; });
});

services.Configure&lt;GzipCompressionProviderOptions&gt;(options =&gt; 
{
    options.Level = CompressionLevel.Fastest;
